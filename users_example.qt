Users = class {
   __init = {
      __self?[users] = users? or {};
   };
   __text = {
      __self?.users or 3;
   };
   add = {
      last_pos = len(type=S, __self?.users)!;
      (__self?.users)$ .S= [last_pos?, to_add?]!;
      # __self?[users] = users? or {};
   };
};

users = new Users?();
users?.add( to_add=5 )!;
disp( users?.users )!;
