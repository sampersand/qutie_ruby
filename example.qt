Car = {
   wheels = 4;
   maker = 'honda?';

   drive = {
      if(dist?){
         disp(`Drove`, dist?, 'miles!');
      } else {
         disp(`vroom vroom!`);
      }
   };
   __init__ = {
      self?[wheels] = num_wheels?; // pre-process :P
   };
};

car = new Car?(num_wheels = 2);
disp(car? : wheels);
car? : drive, @ ();
car?.drive(dist=3);
disp(Car? @ ()!, : wheels)