Car = {
   wheels = 4;
   maker = 'honda?';

   drive = {
      if(dist?){
         disp(`Drove`, dist?, 'miles!');
      } else {
         disp(`vroom vroom!`);
      }
   };
   __init = {
      self?[wheels] = num_wheels?; // pre-process :P
   };
   __str = {
      "I'm a " + self?.wheels
      'a'
   };
};

car = new Car?(num_wheels = 2);
disp(car?.wheels);
disp(car?);
dr=car?.drive;
dr?();
car?.drive(dist=3);
