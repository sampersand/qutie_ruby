
print_data = function(prefix='') {
   prefix |= '';
   disp(prefix?, 'A?: ', $?.-2,.A|_, '\n', prefix?, 'B?: ', $?.-2,.B|_);
};
foo = {
   A = 1;
   print_data?@()!;
   {
      B = 2;
      print_data?@(prefix='\t')!;
      $?.-1, .= (B, 3)!;
      $?.-2, .= (B, 4)!;
      print_data?@(prefix='\t')!;
   }@()!;
   print_data?@()!;
};
