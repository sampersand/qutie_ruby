Heap = {
  elements = []@();
  insert = { -> to_insert;
    if(__self?.elements){
      __self?.elements .= (1, new Node(to_insert?))!;

    } else {
      __self?.elements .= (0, new Node(to_insert?))!;
    }
  };
  __text = {
    '[' + text(__self?.elements) + ']'
  };
};

Node = {
  -> data;
  -> left;
  -> right;
  __text = { text(__self?.data) + if(left? || right?, "{" + left? + ", " + right? + "}", '') };
};

list = new Heap();
list?.insert(0);
list?.insert(1);
disp( list? );











