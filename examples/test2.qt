Car <- {
  maker = maker? || 'honda';
  wheels = wheels? || 4;

  __text = {
    `I'm a ` + __self?.maker + ' with ' + __self?.wheels + ' wheels!'
  };

  drive = {dist=dist?||__args?.0;
    disp? @ ( if? @ (dist?, 'I drove ' + dist? + ' miles!', 'vroom vroom!')! )!;
  };
  __num = {
    __self?.wheels
  };
  __bool = { nil? }
};



car = Car? @ (maker: 'toyoya';)!;
text? @ (car?)!,
num? @ (car?)!,
bool? @ (car?)!
