Fraction <- {
  numer = numer? || __args?.0;
  denom = denom? || __args?.1;
  disp?@(numer?==0)!;
  __text = { __self?.numer + '/' + __self?.denom };
  __num =  { __self?.numer    /    __self?.denom };
  __bool = { __self?.numer == 0 };
  # __num = {
  #   __self?.wheels
  # };
  # __bool = { missing? }
};

car = Fraction? @ (0, 3)!;
text? @ (car?)!,
num? @ (car?)!,
bool? @ (car?)!