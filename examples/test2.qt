Heap = {
  elements = []@();
  __text = { '[' + text(__self?.elements) + ']' };
  __len = { len(__self?.elements) };
  insert = { -> to_insert;
    if(__self?.elements){
      if(__self?.elements.1 == 1 || true?){
        __self?.elements .= (len(__self?)*2, to_insert?)!;
      }

    } else {
      __self?.elements .= (1, to_insert?)!;
    }
  };
};

list = new Heap();
list?.insert(0);
list?.insert(1);
disp( list? );











