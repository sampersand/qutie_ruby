LinkedList = class {
  head ||= nil?;

  __text = function(){
    current = __self?.head;
    res = '[';
    current? == nil? && return(res? + ']');
  };
  unshift = function(val?){
    __self? .= (head, new Node(val?, __self?.head))!;
  };
  shift = function(){
    res = __self?.head;
    __self? .= (head, __self?.head.next)!;
    res?
  };
};

Node = class {
  data = __args?.0;
  next = __args?.1 || nil?;

  __text = function() {
    text(__self?.data)
  };

};

list = new LinkedList();
list?.unshift('d');
list?.unshift('c');
list?.unshift('b');
list?.unshift('a');
disp( list? );
disp( list?.shift().0 );
disp( list? );




