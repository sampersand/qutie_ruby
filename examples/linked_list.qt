LinkedList = class {
  head ||= nil?;
  __text = function(){
    current = __self?.head;
    res = '[';
    if(current? <> nil?, {

      }, {})!;
    while([current? <> nil?], {
      res += text(current?);
      current = current?.next;
    });
    res? + ']'
  }
};

Node = class {
  data = __args?.0;
  next = __args?.1 || nil?;

  __text = function() {
    text(__self?.data)
  };

};

list = new LinkedList();
list? .= (head, new Node('a'))!;
