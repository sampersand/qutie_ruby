  #[DISP_TEXT]
Car = {
  maker  = maker? || honda;
  wheels = wheels? || 4;

  __text = {
    disp(type(__self?))
    'I\'m a ' + __self?.maker + ' with ' + __self?.wheels + ' wheels!'
  };

  drive = { 
    dist = __args?.0 || __args?.dist;
    disp( if(dist?,  
             'I drove ' + dist? + ' miles!',
             'vroom vroom!')
        );
  };

};

car = new Car(maker: toyota;)!;
disp( car? );
car?.drive( 5 );
car?.drive( dist=9.3 );
car?.drive();
