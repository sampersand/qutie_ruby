array = [1, 2, 3, 4]

Car = {
  maker  = maker? || honda;
  wheels = wheels? || 4;

  __text = {
    'I\'m a ' + __self?.maker + ' with ' + __self?.wheels + ' wheels!'
  };

  drive = { 
    dist = __args?.0 || __args?.dist;
    disp( if(dist?,  
             'I drove ' + dist? + ' miles!',
             'vroom vroom!')
        );
  };

};

car = new Car(maker: toyoya);
disp( text(car?) );
